{
  "name": "@functionland/box",
  "version": "0.6.4",
  "description": "Reference implementation of box server in Node.js",
  "main": "dist/index.js",
  "bin": "./cli.js",
  "types": "src",
  "scripts": {
    "start": "nodemon --watch dist",
    "build": "node scripts/build.mjs",
    "build:watch": "node scripts/build.mjs",
    "lint": "eslint src --fix",
    "test": "node -r esbuild-runner/register node_modules/tape/bin/tape tests/**/*.test.{ts,js} | tap-spec",
    "test:orbitDB": "node -r esbuild-runner/register node_modules/tape/bin/tape tests/**/orbitDB.test.{ts,js} | tap-spec",
    "test:graphql": "node -r esbuild-runner/register node_modules/tape/bin/tape tests/**/graphql.test.{ts,js} | tap-spec",
    "test:sel": "node -r esbuild-runner/register node_modules/tape/bin/tape $TEST_PATH | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/functionland/box.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/functionland/fula/issues"
  },
  "homepage": "https://github.com/functionland/fula#readme",
  "dependencies": {
    "@functionland/file-protocol": "~0.3.6",
    "@functionland/graph-protocol": "~0.2.2",
    "@ipld/dag-json": "~8.0.8",
    "async-later": "0.1.11",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "graphql-tools": "8.2.0",
    "graphql-type-json": "0.3.2",
    "it-pipe": "~2.0.3",
    "libp2p": "~0.37.3",
    "@chainsafe/libp2p-noise": "^6.0.1",
    "@libp2p/peer-id-factory": "^1.0.10",
    "@libp2p/bootstrap": "^1.0.2",
    "@libp2p/mplex": "^1.0.5",
    "@libp2p/webrtc-star": "^1.0.12",
    "@libp2p/websockets": "^1.0.8",
    "@libp2p/tcp": "~1.0.11",
    "@libp2p/peer-id": "^1.1.10",
    "@libp2p/delegated-peer-routing": "^2.0.1",
    "multiformats": "~9.7.0",
    "openpgp": "~5.2.0",
    "orbit-db": "~0.28.3",
    "streaming-iterables": "~7.0.4",
    "wrtc": "~0.4.7",
    "config": "~3.3.7",
    "ipfs-http-client": "~56.0.3",
    "@libp2p/interfaces": "~3.0.2",
    "default-gateway": "^6.0.2",
    "ipfs-utils": "^9.0.6",
    "leveldown": "^5.0.0",
    "@achingbrain/ssdp":"^4.0.1"
  },
  "devDependencies": {
    "@types/debug": "~4.1.7",
    "node-pre-gyp": "^0.17.0",
    "@types/tape": "~4.13.2",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.13.0",
    "@web-std/file": "~3.0.2",
    "debug": "~4.3.3",
    "esbuild": "~0.14.47",
    "esbuild-runner": "~2.2.1",
    "eslint": "~8.10.0",
    "nodemon": "~2.0.15",
    "nyc": "~15.1.0",
    "tap-spec": "~5.0.0",
    "tape": "~5.5.2",
    "ts-node": "~10.6.0",
    "typescript": "~4.6.2",
    "@types/config": "~0.0.41"
  },
  "engines": {
    "node": ">=16.13.2"
  }
